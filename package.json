{
  "name": "react-material-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate with Material Design",
  "main": "app/main.jsx",
  "scripts": {
    "lint": "eslint 'app/**/*.@(js|jsx)'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --devtool eval --progress --hot --colors --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "validate": "npm ls"
  },
  "dependencies": {
    "babel-runtime": "6.x",
    "react": "15.x",
    "react-dom": "15.x",
    "normalize.css": "4.x",
    "mdi": "1.x",
    "material-ui": "0.x"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "copy-webpack-plugin": "2.x",
    "css-loader": "~0.23.0",
    "eslint": "2.x",
    "eslint-config-airbnb": "7.x",
    "eslint-plugin-jsx-a11y": "0.x",
    "eslint-plugin-react": "4.x",
    "postcss": "5.x",
    "postcss-loader": "0.x",
    "postcss-import": "8.x",
    "autoprefixer": "6.x",
    "style-loader": "0.x",
    "css-loader": "0.x",
    "webpack": "1.x",
    "webpack-dev-server": "1.x"
  },
